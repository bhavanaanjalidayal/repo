package Java123;

public class Binary {

	public static void main(String args[])
	{
		int y = 300;
		System.out.println(Integer.toBinaryString(y));
		char t[] = Integer.toBinaryString(y).toCharArray();
		int temp=0;
		for(int i =0;i<t.length;i++)
		{
			
		if(t[i]==1)
		{
			temp++;
		}
		
		}
		System.out.println(temp);
	}
	
	
	
}
