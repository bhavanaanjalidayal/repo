
public class Operations {

	Operations()
	{
		
	}
	static int d;
	public static int Addition(int a, int b)
	{
		//int c = a+b;
		//System.out.println(a+b);
		return b;
		
	}
	
	public void Subtraction(int d,int c)
	{
		d=d-c;
		System.out.println(d-c);
	 	}
	
	
	public int Multiplication(int s,int y)
	{
		int r = s*y;
		return r;
	}
	public static void main(String a[])
	{
		
		int y = Addition(10,90);
		//System.out.println("d ka value "+d);
		Operations object = new Operations();
		int r = object.Multiplication(10, 2);
		System.out.println(y+r);
		Addition(20,4);
		object.Subtraction(100, 30);
		int f= object.Addition(100, 30);
		System.out.println();
	}
	
}
