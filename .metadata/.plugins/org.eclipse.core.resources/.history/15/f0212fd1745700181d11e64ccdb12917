package appModule;

import org.openqa.selenium.By;
import org.openqa.selenium.WebDriver;

import Xcelerator.main.com.testproject.java.framework.locators.*;

public class HomePageScripts {
	static HomePage wb = new HomePage();
	static WebDriver driver = wb.WebBrowser("Chrome");
	//void launchURL()
	public static void main(String args[])
	{
		driver.get("https://xcelerator.ninja");
	}
	
	void clickon3Lines()
	{
		driver.findElement(By.xpath("//span[@class='hamBurger']/i[@class='fa fa-bars']")).click();
		
	}
}
